{
  "name": "react_native_expo_tutorial-monorepo",
  "private": true,
  "version": "1.0.0",
  "packageManager": "pnpm@10.25.0",
  "pnpm": {
    "overrides": {
      "react-test-renderer": "19.1.0",
      "jest-expo>react-test-renderer": "19.1.0"
    }
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "dotenv-cli": "^11.0.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.2.6",
    "prettier": "^3.0.0"
  },
  "scripts": {
    "test": "jest",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,css,scss,html}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,css,scss,html}\"",
    "mobile:start": "pnpm -F @app/mobile start",
    "mobile:start:dev": "pnpm -F @app/mobile start:dev",
    "mobile:start:stg": "pnpm -F @app/mobile start:stg",
    "mobile:start:prod": "pnpm -F @app/mobile start:prod",
    "mobile:run:ios": "pnpm -F @app/mobile run:ios",
    "mobile:run:ios:dev": "pnpm -F @app/mobile run:ios:dev",
    "mobile:run:ios:stg": "pnpm -F @app/mobile run:ios:stg",
    "mobile:run:ios:prod": "pnpm -F @app/mobile run:ios:prod",
    "mobile:run:android": "pnpm -F @app/mobile run:android",
    "mobile:run:android:dev": "pnpm -F @app/mobile run:android:dev",
    "mobile:run:android:stg": "pnpm -F @app/mobile run:android:stg",
    "mobile:run:android:prod": "pnpm -F @app/mobile run:android:prod",
    "mobile:ios:launch:dev": "pnpm -F @app/mobile ios:launch:dev",
    "mobile:build:ios:dev": "pnpm -F @app/mobile build:ios:dev",
    "mobile:build:ios:stg": "pnpm -F @app/mobile build:ios:stg",
    "mobile:build:ios:prod": "pnpm -F @app/mobile build:ios:prod",
    "mobile:build:android:dev": "pnpm -F @app/mobile build:android:dev",
    "mobile:build:android:stg": "pnpm -F @app/mobile build:android:stg",
    "mobile:build:android:prod": "pnpm -F @app/mobile build:android:prod",
    "mobile:test": "pnpm -F @app/mobile test",
    "mobile:lint": "pnpm -F @app/mobile lint",
    "web:dev": "pnpm -F @app/web dev",
    "web:stg": "pnpm -F @app/web stg",
    "web:prod": "pnpm -F @app/web prod",
    "web:build": "pnpm -F @app/web build",
    "web:start": "pnpm -F @app/web start",
    "web:lint": "pnpm -F @app/web lint",
    "core:openapi:bundle": "pnpm -F @app/core openapi:bundle",
    "core:generate:types": "pnpm -F @app/core generate:types",
    "core:generate:schemas": "pnpm -F @app/core generate:schemas",
    "core:generate:mocks": "pnpm -F @app/core generate:mocks",
    "core:generate:all": "pnpm -F @app/core generate:all",
    "core:swagger": "pnpm -F @app/core swagger",
    "core:mock:server": "pnpm -F @app/core mock:server"
  }
}
