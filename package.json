{
  "name": "react_native_expo_tutorial",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "start:ios-dev": "APP_VARIANT=develop expo start --ios",
    "start:ios-stg": "APP_VARIANT=staging expo start --ios",
    "start:ios-prod": "APP_VARIANT=production expo start --ios",
    "start:android-dev": "APP_VARIANT=develop expo start --android",
    "start:android-stg": "APP_VARIANT=staging expo start --android",
    "start:android-prod": "APP_VARIANT=production expo start --android",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "run:android:dev": "APP_VARIANT=develop expo run:android",
    "run:android:stg": "APP_VARIANT=staging expo run:android",
    "run:android:prod": "APP_VARIANT=production expo run:android",
    "ios": "expo run:ios",
    "run:ios-dev": "APP_VARIANT=develop expo run:ios",
    "run:ios-stg": "APP_VARIANT=staging expo run:ios",
    "run:ios-prod": "APP_VARIANT=production expo run:ios",
    "web": "expo start --web",
    "build:dev": "APP_VARIANT=develop eas build --profile develop",
    "build:staging": "APP_VARIANT=staging eas build --profile staging",
    "build:prod": "APP_VARIANT=production eas build --profile production",
    "build:ios-dev": "APP_VARIANT=develop eas build --profile develop --platform ios",
    "build:ios-stg": "APP_VARIANT=staging eas build --profile staging --platform ios",
    "build:ios-prod": "APP_VARIANT=production eas build --profile production --platform ios",
    "build:android-dev": "APP_VARIANT=develop eas build --profile develop --platform android",
    "build:android-stg": "APP_VARIANT=staging eas build --profile staging --platform android",
    "build:android-prod": "APP_VARIANT=production eas build --profile production --platform android",
    "submit:ios-dev": "eas submit --profile develop --platform ios",
    "submit:ios-stg": "eas submit --profile staging --platform ios",
    "submit:ios-prod": "eas submit --profile production --platform ios",
    "lint": "expo lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "generate:types": "orval --config orval.config.ts && node scripts/organize-types.js",
    "generate:schemas": "node scripts/generate-schemas.js",
    "generate:mocks": "node scripts/generate-mock-data.js",
    "generate:all": "npm run generate:types && npm run generate:schemas && npm run generate:mocks",
    "swagger": "node scripts/swagger-server.js",
    "mock:server": "prism mock openapi/api-spec.yaml --port 4010"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@tanstack/react-query": "^5.90.11",
    "axios": "^1.13.2",
    "dotenv": "^17.2.3",
    "expo": "~54.0.20",
    "expo-constants": "~18.0.10",
    "expo-dev-client": "~6.0.16",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.13",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.8",
    "expo-updates": "~29.0.12",
    "expo-web-browser": "~15.0.8",
    "immer": "^10.2.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "zod": "^3.25.76",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@orval/core": "^7.17.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.0",
    "babel-jest": "^30.2.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-preset-expo": "^54.0.7",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "express": "^5.2.1",
    "jest": "^30.2.0",
    "jest-expo": "^54.0.13",
    "orval": "^7.17.0",
    "swagger-ui-express": "^5.0.1",
    "ts-to-zod": "^5.0.1",
    "typescript": "~5.9.2",
    "yamljs": "^0.3.0"
  },
  "private": true
}
