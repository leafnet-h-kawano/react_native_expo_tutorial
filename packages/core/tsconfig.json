{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react-jsx",
    "moduleResolution": "Bundler",
    "module": "ESNext",
    "paths": {
      "@core/*": ["./*"]
    },
    "composite": false,
    "declaration": false,
    "emitDeclarationOnly": false,
    "noEmit": true
  },
  "include": ["./**/*.ts", "./**/*.tsx"]
}
