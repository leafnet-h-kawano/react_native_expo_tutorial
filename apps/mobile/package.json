  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "APP_VARIANT=develop expo start --dev-client",
    "start:dev": "APP_VARIANT=develop expo start --dev-client",
    "start:stg": "APP_VARIANT=staging expo start --dev-client",
    "start:prod": "APP_VARIANT=production expo start --dev-client",
