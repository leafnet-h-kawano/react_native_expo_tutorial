    "build:ios:dev": "APP_VARIANT=develop eas build --profile develop --platform ios",
    "build:ios:stg": "APP_VARIANT=staging eas build --profile staging --platform ios",
    "build:ios:prod": "APP_VARIANT=production eas build --profile production --platform ios",
    "build:android:dev": "APP_VARIANT=develop eas build --profile develop --platform android",
    "build:android:stg": "APP_VARIANT=staging eas build --profile staging --platform android",
    "build:android:prod": "APP_VARIANT=production eas build --profile production --platform android",
